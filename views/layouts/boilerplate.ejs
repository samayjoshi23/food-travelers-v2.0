    <link rel="shortcut icon" href="/static/images/favicon.ico">
    <link rel="icon" type="image/gif" href="/static/images/animated_favicon1.gif">
    <link rel="stylesheet" href="/static/stylesheets/nav-bar.css">
    <link rel="stylesheet" href="/static/stylesheets/footer.css">
    <style>
        *,
        p,
        h1,
        h2,
        h3,
        h4,
        h5,
        h6 {
            padding: 0;
            margin: 0;
            box-sizing: border-box;
        }

        ::-webkit-scrollbar {
            width: 0px !important;
        }

        :root {
            --state-card-m-p-pc-tab: 4%;
            --main-sections-margin-pc: 2% auto;
            --main-sections-margin-tab: 2% auto;
            --main-sections-width-pc: 90%;
            --main-sections-width-tab: 95%;
            --main-sections-width-mob: 98%;
            --def-border-radius: 5px;
            --def-box-shadow-lg: 1px 0 5px 0 rgb(122, 122, 122);
            --def-box-shadow-sm: 1px 0 4px 0 rgb(172, 172, 172);
            --def-linear-gradient-hor: linear-gradient(to right,
                    rgba(0, 0, 0, 0),
                    rgba(0, 0, 0, 0.1),
                    rgba(0, 0, 0, 0.3),
                    rgba(0, 0, 0, 0.1),
                    rgba(0, 0, 0, 0)) 1;
            --def-linear-gradient-ver: linear-gradient(rgba(0, 0, 0, 0),
                    rgba(0, 0, 0, 0.1),
                    rgba(0, 0, 0, 0.3),
                    rgba(0, 0, 0, 0.1),
                    rgba(0, 0, 0, 0)) 1;

            --font-main-heading: "Bakbak One", cursive;
            --font-sub-heading: "Baloo Bhaina 2", cursive;
            --font-content: "Montserrat", sans-serif;
        }
    </style>
    <link rel="stylesheet" href="/static/stylesheets/<%-css%>">
</head>

